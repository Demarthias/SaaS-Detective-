{
  "manifest_version": 3,
  "name": "SaaS Detective",
  "version": "1.0.0",
  "description": "Identify the technology stack behind any website.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}
