{
  "manifest_version": 3,
  "name": "SaaS Stack Detective",
  "version": "1.1",
  "author": "Venom Industries",
  "description": "Automatically identifies and catalogs SaaS usage to optimize costs and security.",
  "permissions": [
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  "action": {
    "default_popup": "popup.html"
  }
}
